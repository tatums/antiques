<% if current_user %>
  <script language="javascript">
    $(document).ready(function(){
      $('#products').sortable({
        update: function(event, ui) {
          var ProductsOrder = $(this).sortable('toArray');
          $.post( 'products/sort', {ProductsOrder:ProductsOrder});
        }
      });
    });
  </script>
<% end %>

<%= render 'categories/products' %>